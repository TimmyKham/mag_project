{{ form_start(form) }}
<div class="row">
    <div class="col-md-4">{{ form_row(form.title) }}</div>
    <div class="col-md-2">{{ form_row(form.category) }}</div>

</div>
<div class="row">
    <div class="col-md-2">{{ form_row(form.price) }}</div>
    <div class="col-md-2">{{ form_row(form.seller) }}</div>
    <div class="col-md-2">{{ form_row(form.imageFile) }}</div>
</div>
<div class="row">
    <div class="col-md-6">{{ form_row(form.description) }}</div>

</div>
{{ form_widget(form) }}
<button class="btn btn-primary" style="background-color:#be1c33!important; " >{{button|default('Enregistrer')}}</button>
{{ form_end(form) }}